<h1>
  Add a new photo
</h1>


<hr>

<!-- Form photo start -->
<form action="/create_photo" method="post">
<!-- Form photo end -->

  <!-- Label and input for image_url start -->
  <div class="form-group">
    <label for="image_url">
      Image url
    </label>

    <!-- Input for image_url start -->
    <input type="text" id="image_url" name="image_url" class="form-control">
    <!-- Input for image_url end -->
  </div>
  <!-- Label and input for image_url end -->

  <button class="btn btn-block btn-outline-secondary">
    Create photo
  </button>
</form>

<hr>

<a href="/photos">
  Go back
</a>
